#. type: Title #
#: ../_i18n/en/resources/user-guides/vps_run_node.md:3
#, markdown-text, no-wrap
msgid "monerod"
msgstr ""

#. type: Plain text
#: ../_i18n/en/resources/user-guides/vps_run_node.md:6
#, markdown-text
msgid "`monerod` is the @daemon software that ships with the Monero tree. It is a console program, and manages the blockchain. While a bitcoin wallet manages both an account and the blockchain, Monero separates these: `monerod` handles the blockchain, and `monero-wallet-cli` handles the account."
msgstr ""

#. type: Plain text
#: ../_i18n/en/resources/user-guides/vps_run_node.md:8
#, markdown-text
msgid "This guide assumes you have already set up your VPS account and are using SSH to tunnel into the server console."
msgstr ""

#. type: Title ##
#: ../_i18n/en/resources/user-guides/vps_run_node.md:9
#, markdown-text, no-wrap
msgid "Linux, 64-bit (Ubuntu 16.04 LTS)"
msgstr ""

#. type: Title ###
#: ../_i18n/en/resources/user-guides/vps_run_node.md:11
#, markdown-text, no-wrap
msgid "Make sure that port 18080 is open"
msgstr ""

#. type: Plain text
#: ../_i18n/en/resources/user-guides/vps_run_node.md:14
#, markdown-text
msgid "`monerod` uses this port to communicate with other nodes on the Monero network."
msgstr ""

#. type: Plain text
#: ../_i18n/en/resources/user-guides/vps_run_node.md:17
#, markdown-text
msgid "Example if using `ufw`: `sudo ufw allow 18080` Example if using `iptables`: `sudo iptables -A INPUT -p tcp --dport 18080 -j ACCEPT`"
msgstr ""

#. type: Title ###
#: ../_i18n/en/resources/user-guides/vps_run_node.md:18
#, markdown-text, no-wrap
msgid "Download the current Monero Core binaries"
msgstr ""

#. type: Plain text
#: ../_i18n/en/resources/user-guides/vps_run_node.md:21
#, markdown-text, no-wrap
msgid "    wget https://downloads.getmonero.org/linux64\n"
msgstr ""

#. type: Title ###
#: ../_i18n/en/resources/user-guides/vps_run_node.md:22
#, markdown-text, no-wrap
msgid "Make a directory and extract the files."
msgstr ""

#. type: Plain text
#: ../_i18n/en/resources/user-guides/vps_run_node.md:26
#, markdown-text, no-wrap
msgid ""
"    mkdir monero\n"
"    tar -xjvf linux64 -C monero\n"
msgstr ""

#. type: Title ###
#: ../_i18n/en/resources/user-guides/vps_run_node.md:27
#, markdown-text, no-wrap
msgid "Launch the daemon"
msgstr ""

#. type: Plain text
#: ../_i18n/en/resources/user-guides/vps_run_node.md:31
#, markdown-text, no-wrap
msgid ""
"    cd monero\n"
"    ./monerod\n"
msgstr ""

#. type: Title ###
#: ../_i18n/en/resources/user-guides/vps_run_node.md:32
#, markdown-text, no-wrap
msgid "Options:"
msgstr ""

#. type: Plain text
#: ../_i18n/en/resources/user-guides/vps_run_node.md:35
#, markdown-text
msgid "Show list of all options and settings:"
msgstr ""

#. type: Plain text
#: ../_i18n/en/resources/user-guides/vps_run_node.md:37
#, markdown-text, no-wrap
msgid "    ./monerod --help\n"
msgstr ""

#. type: Plain text
#: ../_i18n/en/resources/user-guides/vps_run_node.md:39
#, markdown-text
msgid "Launch the daemon as a background process:"
msgstr ""

#. type: Plain text
#: ../_i18n/en/resources/user-guides/vps_run_node.md:41
#, markdown-text, no-wrap
msgid "    ./monerod --detach\n"
msgstr ""

#. type: Plain text
#: ../_i18n/en/resources/user-guides/vps_run_node.md:43
#, markdown-text
msgid "Monitor the output of `monerod` if running as daemon:"
msgstr ""

#. type: Plain text
#: ../_i18n/en/resources/user-guides/vps_run_node.md:45
#, markdown-text, no-wrap
msgid "    tail -f ~/.bitmonero/bitmonero.log\n"
msgstr ""

